//2
#include<iostream>
#include <math.h>
#include <conio.h>

using namespace std;

int factorial(int i)
{
  if (i==0) return 1;
  else return i*factorial(i-1);
}
 
int main() 
{
    float s, n;
    int i, k;
    
    cout << "n=";
    cin >> n;
   
    s = 0; k = 1;
    do {
        k++;
        s=s+pow(2,k)/ factorial(k);    
    } 
    while (k <= n);
    cout << "s=" << s <<"\n"; 
}

