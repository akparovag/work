#include <stdio.h>
#include <iostream>
using namespace std;


int main() //factorial
{
    int n, i;
    long double p ;
    
    cout << "n = ";
    cin >> n;
    p=1;
    for(i = 1; i <= n; i++){
        p =p* i;
    }
    cout << "p = "<< p << "\n";
    return 0;
}

#include <iostream>
#include <string>
using namespace std;

int main()
{
  int s = 4;
  
  if(s % 2 == 0)
    cout<<"жуп";
  else
    cout<<"так";
  
}
